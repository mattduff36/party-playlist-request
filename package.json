{
  "name": "party-playlist-request",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:https": "node server-https.js",
    "dev:https:real": "cross-env FORCE_REAL_SPOTIFY=true node server-https.js",
    "generate-certs": "node generate-certs.js",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "type-check": "tsc --noEmit",
    "clean": "rimraf .next node_modules/.cache",
    "dev:debug": "NODE_OPTIONS='--inspect' next dev --turbopack",
    "test:local": "node test-local.js",
    "test:auth": "npm run dev & echo 'Testing authentication flow at http://localhost:3000/admin'",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:migrate-data": "tsx src/lib/db/migrations/run-migration.ts",
    "db:migrate-dry-run": "tsx src/lib/db/migrations/run-migration.ts --dry-run",
    "db:validate-migration": "tsx src/lib/db/migrations/run-migration.ts --validate-only",
    "db:create-indexes": "tsx src/lib/db/indexes.ts create",
    "db:analyze-performance": "tsx src/lib/db/indexes.ts analyze",
    "db:create-constraints": "tsx src/lib/db/constraints.ts create",
    "db:validate-data": "tsx src/lib/db/constraints.ts validate",
    "test": "jest",
    "test:unit": "node scripts/test-runner.js unit",
    "test:integration": "node scripts/test-runner.js integration",
    "test:components": "node scripts/test-runner.js components",
    "test:all": "node scripts/test-runner.js all",
    "test:watch": "node scripts/test-runner.js all --watch",
    "test:coverage": "node scripts/test-runner.js all --coverage",
    "test:comprehensive": "node scripts/test-runner.js integration --coverage",
    "test:load": "node scripts/load-test.js",
    "test:load:basic": "node scripts/load-test-runner.js basicLoad",
    "test:load:spike": "node scripts/load-test-runner.js spikeTest",
    "test:load:stress": "node scripts/load-test-runner.js stressTest",
    "test:load:endurance": "node scripts/load-test-runner.js enduranceTest",
    "test:performance": "npm test -- --config jest.config.integration.js --testPathPatterns=src/__tests__/performance.test.ts"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@neondatabase/serverless": "^1.0.1",
    "@types/qrcode": "^1.5.5",
    "@upstash/redis": "^1.35.4",
    "@vercel/kv": "^3.0.0",
    "axios": "^1.11.0",
    "bcryptjs": "^2.4.3",
    "drizzle-kit": "^0.31.5",
    "drizzle-orm": "^0.44.5",
    "jsonwebtoken": "^9.0.2",
    "jwt-decode": "^4.0.0",
    "lucide-react": "^0.543.0",
    "next": "15.5.2",
    "next-auth": "^4.24.11",
    "pg": "^8.11.0",
    "pusher": "^5.2.0",
    "pusher-js": "^8.4.0",
    "qrcode": "^1.5.4",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.55.0",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/bcryptjs": "^2.4.0",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.0",
    "@types/node": "^20",
    "@types/pg": "^8.10.0",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/uuid": "^10.0.0",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "concurrently": "^9.2.1",
    "cross-env": "^10.0.0",
    "eslint": "^9",
    "eslint-config-next": "15.5.2",
    "https-localhost": "^4.7.1",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "jsdom": "^27.0.0",
    "playwright": "^1.55.0",
    "rimraf": "^6.0.1",
    "selfsigned": "^3.0.1",
    "tailwindcss": "^4",
    "ts-jest": "^29.4.4",
    "tsx": "^4.20.6",
    "typescript": "^5"
  }
}
